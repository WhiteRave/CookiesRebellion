<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Start">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.630"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/__3"
        app:tint="?android:attr/textColorPrimaryNoDisable" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        app:tint="?android:attr/textColorPrimaryNoDisable"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.630"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/__2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.630"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/__"
        app:tint="?android:attr/textColorPrimaryNoDisable" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        app:tint="?android:attr/textColorPrimaryNoDisable"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.630"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/__1"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_hide_splash"
        android:layout_width="364dp"
        android:layout_height="58dp"
        android:layout_gravity="center"
        android:layout_marginBottom="88dp"
        android:backgroundTint="#6337B5"
        android:fontFamily="@font/inter"
        android:text="Продолжить   ❯"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="240dp"
        android:layout_height="100dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/inter_semibold"
        android:text="Добро пожаловать в Bucket List"
        android:textAlignment="center"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="56dp"
        android:fontFamily="@font/inter_semibold"
        android:gravity="center_vertical"
        android:text="Напоминания о задачах"
        android:textAlignment="center"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="56dp"
        android:fontFamily="@font/inter_semibold"
        android:gravity="center_vertical"
        android:text="Пользовательские темы"
        android:textAlignment="center"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="56dp"
        android:gravity="center_vertical"
        android:fontFamily="@font/inter_semibold"
        android:text="Мотивация на каждый день"
        android:textAlignment="center"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="56dp"
        android:fontFamily="@font/inter_semibold"
        android:text="Создавайте Chek List-ы быстро и легко"
        android:textAlignment="center"
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>